sample S.
sample T.

input [1, S, T] in G.

oracle o1(x:Fp) = sample A; return [A, A*T, A*S + x*A*S*T, x*A, x*A*T] in G.

win (A1:G, A2:G, A3:G, A4:G, A5:G, xx:Fp) =
  ( A2 - A1*T = 0 /\ A3 - A1*S - xx*A1*S*T = 0 /\ A4 - xx*A1 = 0 /\ A5 - xx*T*A1 = 0
    /\ forall i: x_i <> xx /\ A1 <> 0 /\ forall i: x_i <> 0 /\ xx <> 0).
